{
    "version": 2,
    "builds": [
        {
            "src": "mirrorweb/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/mirrorweb/mirrorwebapp/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/mirrorweb/wsgi.py"
        }
    ]
}