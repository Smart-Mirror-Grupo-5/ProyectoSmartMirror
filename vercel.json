{
    "version": 2,
    "builds": [
        {
            "src": "mirrorweb/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/mirrorwebapp/static/(.*)",
            "dest": "mirrorwebapp/static/mirrorwebapp/$1"
        },
        {
            "src": "/(.*)",
            "dest": "mirrorweb/wsgi.py"
        }
    ]
}
